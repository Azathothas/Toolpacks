          sudo ln -s "$ANDROID_NDK/toolchains/llvm/prebuilt/linux-x86_64/bin/llvm-ar" "/usr/local/bin/aarch64-linux-android-ar" 2>/dev/null
          sudo chmod +xwr "/usr/local/bin/aarch64-linux-android-ar" 2>/dev/null
